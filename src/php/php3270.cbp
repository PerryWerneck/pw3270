<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="php3270" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output=".bin/Debug/php3270" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="1" />
				<Option object_output=".obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="-d extension=$PWD/.bin/Debug/php3270.so sample.php" />
				<Option host_application="php" />
				<Option run_host_application_in_terminal="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DDEBUG=1" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output=".bin/Release/php3270" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="1" />
				<Option object_output=".obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DNDEBUG=1" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
			<Add option="`php-config --includes`" />
			<Add option="`pkg-config --cflags dbus-1`" />
			<Add directory="../include" />
		</Compiler>
		<Linker>
			<Add option="`pkg-config --libs dbus-1`" />
		</Linker>
		<Unit filename="../classlib/exception.cc" />
		<Unit filename="../classlib/local.cc" />
		<Unit filename="../classlib/remote.cc" />
		<Unit filename="../classlib/session.cc" />
		<Unit filename="../include/pw3270/class.h" />
		<Unit filename="get.cc" />
		<Unit filename="init.cc" />
		<Unit filename="main.cc" />
		<Unit filename="php3270.h" />
		<Unit filename="sample.php" />
		<Unit filename="set.cc" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
