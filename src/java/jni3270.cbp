<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="pw3270 Java extension" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output=".bin/Debug/jni3270" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="-Djava.library.path=.bin/Debug -cp .bin/java/ testprogram " />
				<Option host_application="java" />
				<Option run_host_application_in_terminal="1" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output=".bin/Release/jni3270" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="-Djava.library.path=.bin/Release -cp .bin/java/ testprogram" />
				<Option host_application="java" />
				<Option run_host_application_in_terminal="1" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="../../configure.ac" />
		<Unit filename="../include/lib3270.h" />
		<Unit filename="../include/pw3270/class.h" />
		<Unit filename="Makefile.in" />
		<Unit filename="actions.cc" />
		<Unit filename="connect.cc" />
		<Unit filename="cursor.cc" />
		<Unit filename="getset.cc" />
		<Unit filename="info.cc" />
		<Unit filename="jni3270.h" />
		<Unit filename="keyboard.cc" />
		<Unit filename="main.cc" />
		<Unit filename="plugin.cc" />
		<Unit filename="private.h" />
		<Unit filename="terminal.java" />
		<Unit filename="testprogram.java" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
