1 ICON @PACKAGE_TARNAME@.ico

VS_VERSION_INFO VERSIONINFO
FILEVERSION     @PACKAGE_MAJOR_VERSION@,@PACKAGE_MINOR_VERSION@,@PACKAGE_MAJOR_RELEASE@,0
PRODUCTVERSION  @PACKAGE_MAJOR_VERSION@,@PACKAGE_MINOR_VERSION@,@PACKAGE_MAJOR_RELEASE@,0

BEGIN

	BLOCK "StringFileInfo"
	
	BEGIN

		BLOCK "080904E4"
		BEGIN
			VALUE "FileDescription",	"@PACKAGE_DESCRIPTION@"
			VALUE "CompanyName",        "Banco do Brasil S.A.\0"
			VALUE "FileVersion",		"@PACKAGE_VERSION@"
			VALUE "InternalName",		"@PACKAGE_NAME@"
			VALUE "LegalCopyright",		"Copyright (c) Banco do Brasil S.A."
			VALUE "OriginalFilename",	"@PACKAGE_TARNAME@.exe"
			VALUE "ProductName",		"@PACKAGE_NAME@"
			VALUE "ProductVersion",		"@PACKAGE_VERSION@-@PACKAGE_MAJOR_RELEASE@.@PACKAGE_MINOR_RELEASE@"
		END
	END

    BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x809, 0x04E4
	END

END



